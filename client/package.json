{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3100",
  "dependencies": {
    "autoprefixer": "8.6.1",
    "autoprefixer-stylus": "^0.14.0",
    "brotli": "^1.3.2",
    "load-awesome-react-components": "^1.0.9",
    "load-awesome-relative": "^0.0.3",
    "moment": "^2.22.2",
    "nib": "^1.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-big-calendar": "^0.19.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "babel-helpers": "^6.24.1",
    "jest": "^23.1.0",
    "react-test-renderer": "^16.4.0"
  },
  "engines": {
    "node": "10.1.0",
    "yarn": "1.7.0"
  },
  "scripts": {
    "build": "yarn styles && react-scripts build && yarn compress",
    "compress": "node scripts/compress-build run",
    "eject": "react-scripts eject",
    "start": "yarn styles:watch & react-scripts start",
    "styles": "stylus -u nib ./src/**/**/*.styl",
    "styles:watch": "stylus -u nib -w ./src/**/**/*.styl",
    "test": "yarn styles && react-scripts test --env=jsdom --no-cache"
  }
}
