{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "autoprefixer": "8.6.1",
    "autoprefixer-stylus": "^0.14.0",
    "load-awesome-react-components": "1.5.0",
    "load-awesome-relative": "^0.0.3",
    "moment": "^2.22.2",
    "nib": "^1.1.2",
    "react": "^16.4.0",
    "react-big-calendar": "^0.19.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "stylus": "^0.54.5"
  },
  "devDependencies": {
    "babel-helpers": "^6.24.1",
    "react-test-renderer": "^16.4.0"
  },
  "engines": {
    "node": "10.1.0",
    "yarn": "1.7.0"
  },
  "scripts": {
    "styles": "stylus -u nib ./src/**/**/*.styl",
    "styles:watch": "stylus -u nib -w ./src/**/**/*.styl",
    "start": "yarn styles:watch & react-scripts start",
    "build": "yarn styles && react-scripts build",
    "test": "yarn styles && react-scripts test --env=jsdom --no-cache",
    "eject": "react-scripts eject"
  }
}
