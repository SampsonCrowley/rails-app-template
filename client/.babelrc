{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 0.25%",
            "not dead"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "external-helpers",
    "transform-object-assign",
    "transform-object-rest-spread",
  ],
  "env": {
    "test": {
      "presets": [
        "env"
      ],
      "plugins": ["transform-runtime"]
    },
    "production": {
      "presets": [
        ["env", { "modules": false }],
        ["minify"]
      ],
    }
  }
}
